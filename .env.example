# Email RL System - Environment Variables
# Copy this file to .env and fill in your values

# API Keys
OPENAI_API_KEY=your_openai_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Database Connection
# PostgreSQL (primary data store)
DATABASE_URL=postgresql://postgres:postgres@localhost:5433/rl_emails

# SurrealDB (graph database for Enron/Gmail)
SURREALDB_ENRON_URL=ws://localhost:8000/rpc
SURREALDB_GMAIL_URL=ws://localhost:8001/rpc
SURREALDB_USER=root
SURREALDB_PASS=root
SURREALDB_NAMESPACE=rl_emails

# Optional: Weights & Biases for experiment tracking
# WANDB_API_KEY=your_wandb_api_key_here
# WANDB_PROJECT=email-rl

# Optional: Hugging Face for model uploads
# HF_TOKEN=your_huggingface_token_here
